<Window x:Class="WMH.View.AlgorithmParametersView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AlgorithmParametersView" Height="300" Width="300">
    <Grid>
        <Button Content="Zapisz" HorizontalAlignment="Left" Margin="31,177,0,0" VerticalAlignment="Top" Width="75" Click="saveParameters"/>
        <TextBox x:Name="textBoxLongMemory" HorizontalAlignment="Left" Height="23" Margin="52,20,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBoxShortMemory" HorizontalAlignment="Left" Height="23" Margin="52,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBoxIterationCriteria" HorizontalAlignment="Left" Height="23" Margin="52,80,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
