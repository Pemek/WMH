<Window x:Class="WMH.View.GraphView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GraphView" Height="300" Width="300">
    <Grid x:Name="gridView">
        <Button x:Name="buttonGenerateGraph" Content="generuj graf" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="generateGraph"/>
        <TextBox x:Name="textBoxVertex" HorizontalAlignment="Left" Height="23" Margin="90,9,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
