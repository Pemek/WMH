<Window x:Class="WMH.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Przeszukiwanie z Tabu" Height="109" Width="514">
    <Grid>
        <Button x:Name="buttonGenerateGraph1" Content="Graf 1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="buttonGraph1Click"/>
        <Button x:Name="buttonGenerateGraph2" Content="Graf 2" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="75" Click="buttonGraph2Click"/>
        <Button Content="start" HorizontalAlignment="Left" Margin="330,10,0,0" VerticalAlignment="Top" Width="75" Click="startAlg"/>
        <Button Content="parametry algorytmu" HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top" Width="155" Click="parametersAlg"/>
        <Button x:Name="buttonResult" Content="Pokaż wynik" HorizontalAlignment="Left" Margin="410,10,0,0" VerticalAlignment="Top" Width="75" Click="showResultGraph"/>
        <ProgressBar x:Name="progressBar1" HorizontalAlignment="Left" Height="12" Margin="10,48,0,0" VerticalAlignment="Top" Width="475"/>

    </Grid>
</Window>
